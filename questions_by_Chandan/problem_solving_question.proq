def process_data(d: dict) -> dict:
    <los>...</los>
    <sol>
    d['int'] = d['int']**2
    d['float'] = int(d['float'])
    d['str'] = d['str'][::-1]
    d['list'] = sum(d['list'])
    d['set'] = max(d['set'])
    return d</sol>
